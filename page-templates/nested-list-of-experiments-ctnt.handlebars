<h1 class="text-h2-lightblue">{{ lab }}</h1>
<div>
	{{#each experiment-sections }}
		<li>
			<span> {{sect-name}} </span>
			<ol id="list-of-experiments">
				{{#each experiments }}
				<li>
					<a href={{ this.link }}>
						{{ this.name }}
					</a>
				</li>
				{{/each}}
			</ol>
		</li>
	{{/each}}
</div>