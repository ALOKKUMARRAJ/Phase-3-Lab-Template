<h1 class="text-h2-lightblue">{{ lab }}</h1>
<div style="padding-bottom: 102px;">
		{{#each experiment-sections }}
		<li>
			<span style="font-size:20px"> {{sect-name}} </span>
			<ol id="list-of-experiments">
				{{#each experiments }}
				<li>
					<a href={{ this.link }}>
						{{ this.name }}
					</a>
				</li>
				{{/each}}
			</ol>
		</li>
		{{/each}}
</div>